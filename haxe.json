{
  "build" : {
    "-D"   : ["no-deprecation-warnings"],
    "-lib" : [
      "stx_pico", "stx_nano", "stx_ds", "stx_fn", "stx_fp", 
      "stx_coroutine", "fletcher", "coconut.data","coconut.ui","spectatory","tink_state",
      "tink_hxx"
    ],
    "-cp" : "src/main/haxe",
    "html" : {
      "-lib"  : [
        "xDOM","coconut.vdom"
      ],
      "-main" : "Main",
      "-js"   : "build/html/index.js"
    },
    "main" : {
      "-main" : "Main",     
      "--interp" : true
    },
    "test" : {
      "-cp" : "src/test/haxe",
      "-L" : ["stx_test"] 
    },
    "unit" : {
      "--macro" : [
        "include('eu.ohmrun.walker',true,['eu.ohmrun.walker.html5'])"
      ]
    }
  },
  "run" : {
    "html" : []
  }
}